<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.production.dindinn">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.production.dindinn.application.DinDinnFood"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true">

        <activity android:name="com.production.dindinn.activities.Home"
            android:theme="@style/SimpleMaterialTheme"
            android:launchMode="standard"
            android:resizeableActivity="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.production.dindinn.activities.Misc"
            android:theme="@style/SimpleMaterialTheme2"
            android:launchMode="standard"
            android:resizeableActivity="false"
            android:screenOrientation="portrait"/>

    </application>

</manifest>